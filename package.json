{
  "name": "markdown-siyuaner",
  "displayName": "Markdown-SiYuaner",
  "description": "一名思源笔记用户的自用 Markdown 插件",
  "version": "0.0.3",
  "publisher": "city945",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.64.0"
  },
  "keywords": ["markdown", "typora", "siyuan", "思源"],

  "repository": {
    "type": "git",
    "url": "https://github.com/castle945/markdown-siyuaner"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onStartupFinished"],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "siyuaner.markdownViewer",
        "displayName": "Markdown Editor",
        "selector": [
          {
            "filenamePattern": "*.md"
          }
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint src --fix",
    "build": "webpack",
    "start": "webpack serve --config webpack.start.js",
    "watch": "webpack --watch",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0"
  }
}
